    "@workspace/eslint-config": "workspace:*",
    "bcrypt": "^6.0.0",
    "drizzle-orm": "0.44.7",
    "pg": "8.16.3",
    "postgres": "^3.4.8",
    "drizzle-kit": "0.31.6",
    "eslint": "^9.39.3",
    "tsdown": "^0.15.12",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
